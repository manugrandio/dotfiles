[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    dc = diff --cached
    dh1 = diff HEAD~1
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    quick-stats = ! /usr/local/bin/git-quick-stats
